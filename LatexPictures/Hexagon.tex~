\documentclass{article}

\usepackage{tikz}
\usetikzlibrary{decorations.markings}
\begin{document}



\begin{tikzpicture}

\draw[  
        decoration={markings, mark=at position 0.55 with {\arrow[scale=4]{latex}}},     postaction={decorate}
        ] (0,0)--(2,0);
\draw[        decoration={markings, mark=at position 0.55 with {\arrow[scale=4]{latex}}},     postaction={decorate}] (0,2)--(2,2);
\draw[dotted,        decoration={markings, mark=at position 0.5 with {\arrow[scale=4]{latex}}, mark=at position 0.6 with {\arrow[scale=4]{latex}}},    postaction={decorate}] (-1,1)--(0,2);
\draw[dotted,        decoration={markings, mark=at position 0.5 with {\arrow[scale=4]{latex}}, mark=at position 0.6 with {\arrow[scale=4]{latex}}},     postaction={decorate}] (2,0)--(3,1);

\draw[dashed,        decoration={markings, mark=at position 0.45 with {\arrow[scale=4]{latex}}, mark=at position 0.55 with {\arrow[scale=4]{latex}}, mark=at position 0.65 with {\arrow[scale=4]{latex}}},     postaction={decorate}] (-1,0)--(0,0);

\draw[dashed,        decoration={markings, mark=at position 0.45 with {\arrow[scale=4]{latex}}, mark=at position 0.55 with {\arrow[scale=4]{latex}}, mark=at position 0.65 with {\arrow[scale=4]{latex}}},     postaction={decorate}] (2,2)--(3,2);




\end{tikzpicture} 


\end{document}
